pragma solidity ^0.8.0;

contract SimpleContract {
    uint public value1;
    string public value2;

    // Constructor to initialize values
    constructor(uint _value1, string memory _value2) {
        value1 = _value1;
        value2 = _value2;
    }

    // Function to update value1
    function updateValue1(uint _value1) public {
        value1 = _value1;
    }

    // Function to update value2
    function updateValue2(string memory _value2) public {
        value2 = _value2;
    }
}
